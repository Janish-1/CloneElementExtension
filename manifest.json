{
  "manifest_version": 2,
  "name": "CloneElement-InstantCSS",
  "version": "2.0",
  "description": "Clone a Webpage Elements and Instantly Apply CSS",
  "permissions": [
    "activeTab",
    "downloads"
  ],
  "content_scripts": [
    {
      "matches": ["*://*/*"],
      "js": ["scrape.js"]
    }
  ],
  "action": {
    "default_popup": "popup.html"
  }
}
